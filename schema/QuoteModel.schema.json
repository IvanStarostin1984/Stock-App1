{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QuoteModel",
  "type": "object",
  "properties": {
    "symbol": { "type": "string" },
    "close": { "type": "number" },
    "deltaPct": { "type": "number" },
    "timestamp": { "type": "string", "format": "date-time" },
    "open": { "type": "number" },
    "high": { "type": "number" },
    "low": { "type": "number" },
    "currency": { "type": "string" }
  },
  "required": ["symbol", "close", "deltaPct", "timestamp"]
}

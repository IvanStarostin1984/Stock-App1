{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UserCredential",
  "type": "object",
  "properties": {
    "email": { "type": "string", "format": "email" },
    "created": { "type": "string", "format": "date-time" },
    "isPro": { "type": "boolean" },
    "hash": { "type": "string" },
    "salt": { "type": "string" }
  },
  "required": ["email", "created", "isPro", "hash", "salt"]
}
